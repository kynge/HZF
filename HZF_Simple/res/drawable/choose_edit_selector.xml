        <shape>
           <stroke android:width="1dp" android:color="#b4b4b4" />
           <solid android:color="#00ffffff"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
           <stroke android:width="1dp" android:color="#b4b4b4" />
           <solid android:color="#00ffffff"/>
        </shape>
    </item>
    <item>
        <shape>
           <stroke android:width="1dp" android:color="#b4b4b4" />
           <solid android:color="#00ffffff"/>
        </shape>
    </item>
</selector>